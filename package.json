{
    "name": "moteur",
    "version": "0.0.8",
    "description": "A tiny Web/TS engine",
    "license": "MIT",
    "homepage": "https://github.com/moteurjs",
    "repository": {
        "type": "git",
        "url": "https://github.com/moteurjs/moteur"
    },
    "bugs": {
        "url": "https://github.com/moteurjs/moteur/issues"
    },
    "keywords": [
        "TypeScript",
        "JavaScript",
        "Web",
        "Game",
        "Engine"
    ],
    "engines": {
        "pnpm": ">=7"
    },
    "packageManager": "pnpm@7.4.1",
    "scripts": {
        "build": "pnpm run build-moteur",
        "build-moteur": "turbo run \"build-moteur\"",
        "build-examples": "turbo run \"build-example\"",
        "package": "pnpm run package-moteur",
        "package-moteur": "ts-node --project tsconfig.node.json tools/package.ts",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.2.0",
        "@types/node": "^20.5.9",
        "prettier": "^3.0.3",
        "ts-node": "^10.9.1",
        "turbo": "^1.10.13",
        "typescript": "^5.2.2"
    }
}
